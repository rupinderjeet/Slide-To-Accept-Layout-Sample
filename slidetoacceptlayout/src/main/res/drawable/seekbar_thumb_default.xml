<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            android:useLevel="false"
            android:tint="@color/slide_seek_bar_thumb">

            <solid android:color="@color/slide_seek_bar_thumb" />
            <corners android:radius="100dp" />
            <size
                android:width="80dp"
                android:height="@dimen/sta_seekbar_height" />
        </shape>

    </item>

    <item
        android:top="-4dp"
        android:bottom="-4dp">

        <bitmap android:src="@drawable/ic_double_right_arrows_9patch"
            android:gravity="center"
            android:tint="@color/white"/>
    </item>

</layer-list>